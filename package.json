{
  "name": "ip-num",
  "version": "0.0.5-pre-alpha",
  "description": "A TypeScript library that represents various internet protocol numbers. It provides ability to perform various IP related operations like parsing, validating etc.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.json",
    "test": "jasmine-ts spec/**/*.ts",
    "test:watch": "npm run test -- -w",
    "test:coverage": "nyc -e .ts -x *.spec.ts jasmine-ts spec/**/*.ts",
    "dprep": "npm run clean && npm run compile && cp package.json ./dist/src && cd ./dist/src && webpack --config ../../webpack.config.js",
    "dpack": "cd ./dist/src && npm pack",
    "test-release": "npm run dprep && npm run dpack",
    "drelease": "cd ./dist/src && npm publish",
    "docs": "typedoc --out ./docs ./src/ --theme minimal --exclude '**/spec/*.ts'",
    "release": "npm run dprep && npm run drelease"
  },
  "nyc": {
    "reporter": [
      "html"
    ]
  },
  "keywords": [
    "ip",
    "ip-num",
    "IP address",
    "Internet Protocol",
    "Ipv4",
    "Ipv6",
    "Asn"
  ],
  "author": "Dadepo Aderemi",
  "license": "MIT",
  "dependencies": {
    "big-integer": "1.6.23"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.53",
    "@types/node": "7.0.39",
    "awesome-typescript-loader": "^3.2.3",
    "jasmine-node": "1.14.5",
    "jasmine-spec-reporter": "4.1.0",
    "jasmine-ts": "0.2.0",
    "nyc": "11.1.0",
    "source-map-loader": "^0.2.1",
    "tsunit.external": "2.0.11",
    "webpack": "^3.5.6",
    "webpack-auto-inject-version": "^1.0.0"
  }
}
